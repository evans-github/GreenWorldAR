<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:id="@+id/vh_treesplanted_rellayout"
    >

    <e.com.greenworldar.customview.SquareImageView
        android:id="@+id/vh_trees_squareimageview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>


    <TextView
        android:id="@+id/vh_trees_nameoftree"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0/1 Unlocked"
        android:layout_below="@+id/vh_trees_squareimageview"
        android:layout_marginTop="8dp"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="16dp"/>


</RelativeLayout>